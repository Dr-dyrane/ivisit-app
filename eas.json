{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev"
    },
    "staging": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "staging",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "development": {
      "android": {
        "track": "internal"
      }
    },
    "staging": {
      "android": {
        "track": "closed"
      }
    },
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
