{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev"
    },
    "staging": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "staging",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "preview",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "preview2": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "preview2",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "closed-testing": {
      "channel": "closed-testing",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "staging": {
      "android": {
        "track": "closed"
      }
    }
  }
}
